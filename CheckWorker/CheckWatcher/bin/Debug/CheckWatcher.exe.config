<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <appSettings>
    <add key="CheckFolder" value="C:\\CheckFolder" />
  </appSettings>
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler,log4net" />
  </configSections>
  <log4net>
    <appender name="LogFileAppender" type="log4net.Appender.FileAppender">
      <file value="E:\CheckLogs\CheckWatcher.log"/>
      <appendToFile value="true"/>
      <layout type="log4net.Layout.PatternLayout">
        <param name="ConversionPattern" value="%d  %-5p %m%n"/>
      </layout>
    </appender>
    <root>
      <level value="ALL" />
    </root>
    <logger name="CheckLogger">
      <appender-ref ref="LogFileAppender"/>
    </logger>
  </log4net>
  <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8" />
  </startup>
  <system.serviceModel>
      <bindings>
          <basicHttpBinding>
              <binding name="BasicHttpBinding_IDbProxy" />
          </basicHttpBinding>
      </bindings>
      <client>
          <endpoint address="http://localhost:58970/DbProxy.svc" binding="basicHttpBinding"
              bindingConfiguration="BasicHttpBinding_IDbProxy" contract="DbProxyReference.IDbProxy"
              name="BasicHttpBinding_IDbProxy" />
      </client>
  </system.serviceModel>
</configuration>
